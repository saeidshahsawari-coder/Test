      "verses": [
        {
          "verse_number": 1,
          "text": "بسم الله الرحمن الرحیم"
        },
        {
          "verse_number": 2,
          "text": "حمد برای الله ست رب العالمین"
        },
        {
          "verse_number": 3,
          "text": "الرحمن رحیم"
        },
        {
          "verse_number": 4,
          "text": "مالک روز دین"
        },
        {
          "verse_number": 5,
          "text": "خودت را می‌پرستیم و خودت را یاری می‌خواهیم"
        },
        {
          "verse_number": 6,
          "text": "هدایتمان کن راه مستقیم"
        },
        {
          "verse_number": 7,
          "text": "راه کسانی که اِنعام کردی برشان غیر خشم گرفته برشان و نه گمراهان"
        }
      ]
    }
  ]
